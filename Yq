--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()

--Main Script
local Window = OrionLib:MakeWindow({Name = "Bouga üé≠", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})


--Valor
_G.Auto7 = True

--Fun√ß√£o
function Auto7()
while _G.Auto7 == true do
local args = {
    [1] = {
        [1] = {
            [1] = "\7"
        }
    },
    [2] = "6ce804d9d7ab412d87bf5df616ad5872"
}

game:GetService("ReplicatedStorage"):WaitForChild("FurryNet"):WaitForChild("FurryEvent"):FireServer(unpack(args))
wait(0.00001)
end
end


