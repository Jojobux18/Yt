--Main
local FarmTab= Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = FarmTab:AddSection({
    Name = "AutoFarm"
})
FarmTab:AddToggle({
    Name = "Auto-Situp",
    Default = false,
    Callback = function(Value)
        _G.Auto7 = Value
        Auto7(0.000001)
    end
})

FarmTab:AddToggle({
    Name = "Auto-Flexao",
